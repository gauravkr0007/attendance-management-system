<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Student</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <div class="container">
      <p><a class="btn btn-primary" href="/admin/students-data/add-student-data-form">Add Student</a>
      </p>
    </div>
    <div class="container">
      <h2>Students</h2>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">S.no</th>
                <th scope="col">Full_name</th>
                <th scope="col">Email</th>
                <th scope="col">Dob</th>
                <th scope="col">Phone No</th>
                <th scope="col">Course</th>
                <th scope="col">Gender</th>
                <th scope="col">Session</th>
                <th scope="col">Section</th>
              </tr>
            </thead>
            <tbody class="table table-striped table-hover">
              <% rows.forEach((eachData) =>{ %>
                <tr>
                  <th scope="row"><%- eachData.stud_id %></th>
                  <td><%- eachData.full_name %></td>
                  <td><%- eachData.email %></td>
                  <td><%- eachData.dob %></td>
                  <td><%- eachData.phone_number %></td>
                  <td><%- eachData.course_name %></td>
                  <td><%- eachData.gender %></td>
                  <td><%- eachData.session %></td>
                  <td><%- eachData.section %></td>
                </tr>
              <% }) %>
              
            </tbody>
          </table>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
